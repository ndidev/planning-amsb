{
  "name": "planning-amsb",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite --host",
    "preview": "vite preview",
    "build:dev": "routify -b && vite build --mode development --sourcemap true --minify false",
    "build:staging": "routify -b && vite build --mode staging --sourcemap true",
    "build:prod": "routify -b && vite build --outDir dist/prod",
    "refresh": "routify -b && vite build --mode development"
  },
  "devDependencies": {
    "@roxi/routify": "^2.18.11",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^4.0.0",
    "@types/autosize": "^4.0.1",
    "@types/hammerjs": "^2.0.41",
    "@types/uuid": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "svelte": "^4.0.0",
    "svelte-preprocess": "^6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "autosize": "^6.0.1",
    "chart.js": "^4.2.1",
    "hammerjs": "^2.0.8",
    "lucide-svelte": "^0.460.1",
    "notiflix": "^3.2.5",
    "purecss": "^3.0.0",
    "svelecte": "^3.15.3",
    "uuid": "^10.0.0"
  },
  "workspaces": [
    "src/lib/**",
    "types/**"
  ]
}
