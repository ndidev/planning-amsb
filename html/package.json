{
  "name": "planning-amsb",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite --host",
    "preview": "vite preview",
    "build:dev": "run-s build:dev:*",
    "build:dev:routify": "routify -b",
    "build:dev:vite": "vite build --mode development --sourcemap true --minify false --outDir dist/dev",
    "build:staging": "run-s build:staging:*",
    "build:staging:routify": "routify -b",
    "build:staging:vite": "vite build --mode staging --sourcemap true --outDir dist/staging",
    "build:prod": "run-s build:prod:*",
    "build:prod:routify": "routify -b",
    "build:prod:vite": "vite build --outDir dist/prod",
    "refresh": "routify -b && vite build --mode development"
  },
  "devDependencies": {
    "@roxi/routify": "^2.18.11",
    "@sveltejs/vite-plugin-svelte": "^2.4.1",
    "@tsconfig/svelte": "^4.0.0",
    "@types/autosize": "^4.0.1",
    "@types/hammerjs": "^2.0.41",
    "@types/uuid": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.59.0",
    "svelte-preprocess": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "autosize": "^5.0.1",
    "chart.js": "^4.2.1",
    "hammerjs": "^2.0.8",
    "notiflix": "^3.2.5",
    "purecss": "^3.0.0",
    "svelecte": "^3.15.3",
    "uuid": "^9.0.0"
  },
  "workspaces": [
    "src/lib/**",
    "types/**"
  ]
}
